{
  "name": "personnal-website",
  "version": "1.0.0",
  "description": "My personnal website built with Nuxt.js and other awesome tools",
  "author": "Julien Sulpis <julien.sulpis@gmail.com>",
  "private": true,
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "analyze": "nuxt build --analyze",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "lint": "eslint --fix --ext .vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/markdownit": "^1.2.3",
    "axios": "^0.18.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "marked": "^0.5.1",
    "nuxt": "^1.0.0",
    "vuetify": "^1.1.4"
  },
  "devDependencies": {
    "@nuxtjs/google-analytics": "^2.0.2",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-imports": "^1.5.0",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.9.2",
    "push-dir": "^0.4.1",
    "sass-loader": "^7.0.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-style-loader": "^4.1.1"
  }
}
